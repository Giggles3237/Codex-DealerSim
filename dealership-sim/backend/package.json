  "dependencies": {
    "@dealership/shared": "workspace:*",
    "cors": "2.8.5",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "uuid": "9.0.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.15",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.17",
    "@types/supertest": "2.0.16",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
